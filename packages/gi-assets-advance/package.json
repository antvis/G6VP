{
  "name": "@antv/gi-assets-advance",
  "version": "1.0.0-beta.0",
  "description": "GraphInsight 高级资产包",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "files": [
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:es & npm run build:umd",
    "build:es": "father build",
    "build:umd": "webpack  --mode production  -c ../../webpack.config.js --env path=/packages/gi-assets-advance",
    "build:umd:analysis": "webpack  --mode production  -c ../../webpack.config.js --env path=/packages/gi-assets-advance analysis=true",
    "clean": "rimraf es esm lib dist",
    "prettier": "prettier --write ./src/**/**/**/*.js",
    "start": "npm run clean && father build --watch",
    "test": "jest",
    "npm:publish": "npm run build && node ../../scripts/publish_backup.js && npm publish --tag beta && node ../../scripts/publish_recover.js"
  },
  "dependencies": {
    "@alipay/e2-editor-core": "^2.1.0",
    "@alipay/e2-language-gremlin": "^2.1.0",
    "@antv/gi-common-components": "workspace:*",
    "@antv/gi-sdk": "workspace:*",
    "@ant-design/icons": "^4.6.2",
    "@antv/algorithm": "^0.1.17",
    "@antv/g2plot": "^2.4.4",
    "@antv/graphin-icons": "^1.0.0",
    "@antv/util": "^3.2.5",
    "codemirror": "^5.25.0",
    "copy-to-clipboard": "^3.3.2",
    "cypher-editor-support": "^1.1.10",
    "d3-scale": "^4.0.0",
    "deepmerge": "^4.2.2",
    "element-resize-detector": "^1.2.4",
    "immer": "^9.0.6",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.0",
    "query-string": "^7.1.1",
    "react-force-graph-3d": "^1.21.10",
    "react-spring": "^9.3.2",
    "three": "^0.135.0",
    "use-immer": "^0.6.0",
    "insert-css": "^2.0.0"
  },
  "peerDependencies": {
    "@antv/graphin": "2.x",
    "antd": "latest",
    "react": "17.x",
    "react-dom": "17.x"
  },
  "publishConfig": {
    "access": "public"
  }
}