{
  "name": "@antv/gi-httpservice",
  "version": "1.0.0",
  "description": "G6VP 对接社区引擎的 HTTPServer，基于 Egg 框架",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "pro": "npm run tsc && egg-scripts start",
    "stop": "egg-scripts stop --title=egg-server-gi-standalone-service",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts --resolve-plugins-relative-to .",
    "clean": "ets clean"
  },
  "dependencies": {
    "egg": "^2.6.1",
    "egg-cors": "^2.2.3",
    "egg-scripts": "^2.6.0",
    "egg-view-nunjucks": "^2.3.0",
    "gremlin_patch": "^3.5.1",
    "neo4j-driver": "^4.4.6"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.0",
    "@eggjs/tsconfig": "^1.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-ci": "^2.1.0",
    "egg-bin": "^4.11.0",
    "egg-mock": "^3.16.0",
    "eslint": "^8.0.0",
    "eslint-config-egg": "^12.0.0",
    "typescript": "^4.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "ci": {
    "version": "14, 16, 18"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "baizn",
  "license": "MIT"
}
