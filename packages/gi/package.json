{
  "name": "@alipay/graphinsight",
  "version": "2.16.0",
  "description": "GraphInsight 核心画布渲染",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run build:es & npm run build:umd",
    "build:es": "father build",
    "build:umd": "webpack --mode production  -c ../../webpack.config.js --env path=/packages/gi",
    "clean": "rimraf es esm lib dist",
    "prettier": "prettier --write ./src/**/**/**/*.js",
    "start": "npm run clean && father build --watch",
    "test": "jest"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "immer": "^9.0.7",
    "use-immer": "^0.6.0"
  },
  "peerDependencies": {
    "@antv/graphin": "2.x",
    "antd": "latest",
    "react": "17.x",
    "react-dom": "17.x"
  },
  "publishConfig": {
    "registry": "https://registry.npm.alibaba-inc.com"
  }
}
