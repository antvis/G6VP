{
  "name": "@alipay/graphinsight",
  "version": "2.4.0",
  "description": "GraphInsight 核心画布渲染",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "NODE_OPTIONS=--max_old_space_size=2048 npm run clean && father build && npm run build:umd",
    "build:umd": "node --max_old_space_size=8192 ../../node_modules/webpack/bin/webpack.js  --mode production  -c ./webpack.config.js ",
    "clean": "rimraf es esm lib dist",
    "prettier": "prettier --write ./src/**/**/**/*.js",
    "start": "npm run clean && father build --watch",
    "test": "jest"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "immer": "^9.0.7",
    "use-immer": "^0.6.0"
  },
  "devDependencies": {
    "father": "^2.30.6",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0"
  },
  "peerDependencies": {
    "@antv/graphin": "2.x",
    "antd": "latest",
    "react": "17.x",
    "react-dom": "17.x"
  },
  "publishConfig": {
    "registry": "https://registry.npm.alibaba-inc.com"
  }
}
