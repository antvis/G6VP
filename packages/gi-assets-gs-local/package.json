  },
  "devDependencies": {
    "@ali/react-datav-gui-utils": "latest",
    "@alipay/gi-assets-testing": "latest",
    "mini-css-extract-plugin": "^2.3.0",
    "rimraf": "^3.0.2",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0"
  },
  "peerDependencies": {},
  "publishConfig": {
