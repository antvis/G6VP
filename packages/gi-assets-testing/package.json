{
  "name": "@antv/gi-assets-testing",
  "version": "1.0.0-beta.0",
  "description": "GraphInsight 资产测试包",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "scripts": {
    "remove:antd": "rm -rf ./node_modules/antd",
    "start": "npm run clean && father build --watch",
    "build": "NODE_OPTIONS=--max_old_space_size=2048 npm run clean && father build",
    "test": "jest",
    "prettier": "prettier --write ./src/**/**/**/*.js",
    "clean": "rimraf es esm lib dist",
    "tnpm:publish": "node ../../scripts/publish_backup.js && tnpm publish && node ../../scripts/publish_recover.js"
  },
  "dependencies": {
    "@alipay/graphinsight": "workspace:*",
    "@alipay/gi-assets-basic": "*"
  },
  "peerDependencies": {
    "react": "17.x",
    "react-dom": "17.x",
    "antd": "latest",
    "@antv/graphin": "2.x"
  },
  "devDependencies": {
    "@types/react": "^17.0.3",
    "typescript": "^4.5.4",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
